  neighbor 10.216.197.11 peer group FABRIC-PEER_v4
  neighbor 10.216.197.11 remote-as 64517
  !
  neighbor 10.216.197.15 description "DC Fabric Edge Peer V4"
  neighbor 10.216.197.15 peer group FABRIC-PEER_v4
  neighbor 10.216.197.15 remote-as 64517
  !
  neighbor 2001:1000::3 description "DC Fabric Edge Peer V6"
  neighbor 2001:1000::3 peer group FABRIC-PEER_v6
  neighbor 2001:1000::3 remote-as 64517
  !
  neighbor 2001:1000::7 description "DC Fabric Edge Peer V6"
  neighbor 2001:1000::7 peer group FABRIC-PEER_v6
  neighbor 2001:1000::7 remote-as 64517
  !
  neighbor 2001:1000::b description "DC Fabric Edge Peer V6"
  neighbor 2001:1000::b peer group FABRIC-PEER_v6
  neighbor 2001:1000::b remote-as 64517
  !
  neighbor 2001:1000::f description "DC Fabric Edge Peer V6"
  neighbor 2001:1000::f peer group FABRIC-PEER_v6
